.hl-label
  padding: 2px 4px
  border-radius: 3px
  color: $btn-color

  &.default
    background-color: $btn-default-color

  for $type in $color-types
    &.{$type}
      background-color: lookup('$tagsP-' + $type + '-color')

.white
  color: black
  background-color: white
  border: 1px solid black